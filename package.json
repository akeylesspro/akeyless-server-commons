{
    "name": "akeyless-server-commons",
    "version": "1.0.0",
    "scripts": {
        "build:cjs": "tsc --project tsconfig.cjs.json",
        "build:esm": "tsc --project tsconfig.esm.json",
        "build": "powershell -ExecutionPolicy Bypass -File ./ps/build.ps1",
        "update_type": "npm i git+https://github.com/akeylesspro/akeyless-types-commons.git",
        "deploy": "npm run build && git add . && git commit -am '.' && git push",
        "start": "npm run build && node ./dist/cjs/",
        "test": "npm run build && node ./dist/test.js"
    },
    "exports": {
        "./managers": {
            "import": "./dist/managers/index.mjs",
            "require": "./dist/managers/index.js"
        },
        "./middlewares": {
            "import": "./dist/middlewares/index.mjs",
            "require": "./dist/middlewares/index.js"
        },
        "./helpers": {
            "import": "./dist/helpers/index.mjs",
            "require": "./dist/helpers/index.js"
        },
        "./types": {
            "import": "./dist/types/index.mjs",
            "require": "./dist/types/index.js"
        }
    },
    "typesVersions": {
        "*": {
            "managers": [
                "./dist/managers/index.d.ts"
            ],
            "middlewares": [
                "./dist/middlewares/index.d.ts"
            ],
            "helpers": [
                "./dist/helpers/index.d.ts"
            ],
            "types": [
                "./dist/types/index.d.ts"
            ]
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/akeylesspro/akeyless-server-commons.git"
    },
    "optionalDependencies": {
        "axios": "^1.7.7",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "firebase-admin": "^12.4.0",
        "lodash": "^4.17.21",
        "moment": "^2.30.1",
        "moment-timezone": "^0.5.45"
    },
    "devDependencies": {
        "@types/lodash": "^4.17.7",
        "@types/moment": "^2.11.29",
        "@types/node": "^22.5.2",
        "typescript": "^5.5.4"
    },
    "dependencies": {
        "@sendgrid/mail": "^8.1.3",
        "akeyless-types-commons": "github:akeylesspro/akeyless-types-commons",
        "tsup": "^8.3.5"
    }
}
